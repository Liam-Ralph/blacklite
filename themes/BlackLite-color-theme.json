    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#808080"
            }
        },
        {
            "name": "Documentation Comments",
            "scope": [
                "comment.block.documentation",
                "string.quoted.docstring"
            ],
            "settings": {
                "foreground": "#80aaaa"
            }
        },
        {
            "name": "Text Constants",
            "scope": [
                "constant.character",
                "constant.character.escape",
                "constant.regexp"
            ],
            "settings": {
                "foreground": "#90ff90"
            }
        },
        {
            "name": "Language Constant",
            "scope": [
                "constant.language",
                "entity.name.function.preprocessor"
            ],
            "settings": {
                "foreground": "#ff8000"
            }
        },
        {
            "name": "Number",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#9820b0"
            }
        },
        {
            "name": "Unit",
            "scope": [
                "keyword.other.unit"
            ],
            "settings": {
                "foreground": "#cc70bb"
            }
        },
        {
            "name": "String",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#20aa20"
            }
        },
        {
            "name": "Control Keywords",
            "scope": [
                "keyword.control"
            ],
            "settings": {
                "foreground": "#ddaa00"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#dd00aa"
            }
        },
        {
            "name": "Other Keywords",
            "scope": [
                "keyword.other"
            ],
            "settings": {
                "foreground": "#80ccff"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid"
            ],
            "settings": {
                "foreground": "#600000"
            }
        },
        {
            "name": "Variable Type",
            "scope": [
                "storage.type",
                "storage.type.primitive.java",
                "keyword.type"
            ],
            "settings": {
                "foreground": "#20aaff"
            }
        },
        {
            "name": "Function Names and Calls",
            "scope": [
                "entity.name.function",
                "meta.function-call.generic",
                "support.function"
            ],
            "settings": {
                "foreground": "#a040dd"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#bbbbbb"
            }
        },
        {
            "name": "Argument Variables",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#a0a0ff"
            }
        },
        {
            "name": "Separator",
            "scope": [
                "punctuation.separator"
            ],
            "settings": {
                "foreground": "#a0a0a0"
            }
        },
        {
            "name": "Classes and Structs",
            "scope": [
                "entity.name.type"
            ],
            "settings": {
                "foreground": "#ff4038"
            }
        },
        {
            "name": "Placeholders",
            "scope": [
                "constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#ccffcc"
            }
        },
        {
            "name": "Namespace/Imports/Includes",
            "scope": [
                "entity.name.namespace",
                "string.quoted.other"
            ],
            "settings": {
                "foreground": "#3030ff"
            }
        },
        {
            "name": "Markdown Bold/Underline",
            "scope": [
                "markup.bold",
                "markup.underline"
            ],
            "settings": {
                "foreground": "#dddddd"
            }
        },
        {
            "name": "Markdown Italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#909090"
            }
        },
        {
            "name": "Markdown Heading",
            "scope": [
                "markup.heading"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#7040cc"
            }
        },
        {
            "name": "Markdown Heading 1/2",
            "scope": [
                "heading.1.markdown",
                "heading.2.markdown"
            ],
            "settings": {
                "foreground": "#9850c0"
            }
        },
        {
            "name": "Markdown Raw",
            "scope": [
                "markup.inline.raw"
            ],
            "settings": {
                "foreground": "#60ee60"
            }
        },
        {
            "name": "Tag",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#cc6000"
            }
        },
        {
            "name": "Markdown List",
            "scope": [
                "punctuation.definition.list.begin.markdown"
            ],
            "settings": {
                "foreground": "#808080"
            }
        },
        {
            "name": "Markdown Link",
            "scope": [
                "markup.underline.link"
            ],
            "settings": {
                "foreground": "#ddddaa"
            }
        },
        {
            "name": "Markdown Link Name/Description",
            "scope": [
                "string.other.link.title.markdown",
                "string.other.link.description.markdown"
            ],
            "settings": {
                "foreground": "#ccaaff"
            }
        },
        {
            "name": "Property Name",
            "scope": [
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#00a0bb"
            }
        },
        {
            "name": "CSS Class",
            "scope": [
                "entity.other.attribute-name.class.css"
            ],
            "settings": {
                "foreground": "#4040ff"
            }
        },
        {
            "name": "CSS ID",
            "scope": [
                "entity.other.attribute-name.id.css"
            ],
            "settings": {
                "foreground": "#7070ff"
            }
        },
        {
            "name": "Property Value Constant",
            "scope": [
                "support.constant.property-value"
            ],
            "settings": {
                "foreground": "#cc8020"
            }
        },
        {
            "name": "RGB Value",
            "scope": [
                "constant.rgb-value",
                "constant.other.color.rgb-value"
            ],
            "settings": {
                "foreground": "#60dd60"
            }
        },
        {
            "name": "Attribute Name",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#ddcc40"
            }
        },
        {
            "name": "Other Keyword",
            "scope": [
                "keyword.other"
            ],
            "settings": {
                "foreground": "#ee7878"
            }
        },
        {
            "name": "Modifier",
            "scope": [
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#80ccff"
            }
        }
    ]
}
